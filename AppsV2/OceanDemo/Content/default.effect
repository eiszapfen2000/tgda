effect "default":

technique "render"
{
    # effect variables
    uniform mat4 np_modelviewprojectionmatrix;
    # shaders
    set vertex shader "default.vertex"
    set fragment shader "default.fragment"
}

technique "color"
{
    # effect variables
    uniform mat4 np_modelviewprojectionmatrix;
    uniform vec4 color;
    # shaders
    set vertex shader "color.vertex"
    set fragment shader "color.fragment"
}


