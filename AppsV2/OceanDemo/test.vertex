#extension GL_ARB_explicit_attrib_location : enable

layout(location = 0) in vec2 position;
layout(location = 1) in vec2 texcoordsIn;

out vec2 texcoordsOut;

void main()
{
    //gl_TexCoord[0] = vec4(texcoords.xy, 0.0f, 0.0f);
